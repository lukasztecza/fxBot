{
    "applicationStartingPoint": "outputMiddleware",

    "assetsVersion": "1",

    "securityList": [
        {
            "route": "/restricted/*",
            "methods": ["GET"],
            "allow": ["ROLE_USER"]
        },
        {
            "route": "/private/{directory}/{file}",
            "methods": ["GET"],
            "allow": ["ROLE_USER"]
        }
    ],

    "cacheList": [
        {
            "route": "/",
            "time": "10"
        }
    ]
}
