{
    "/admin/{id}": {
        "controller": "user_controller",
        "method": ["GET"],
        "requirements": {"id": "\\d+"},
        "action": "get"
    },
    "/user/{id}/{word}": {
        "controller": "user_controller",
        "method": ["POST", "GET"],
        "requirements": {"id": "\\d+", "word": "\\D+"},
        "action": "get"
    },
    "/": {
        "controller": "user_controller",
        "action": "get",
        "requirements":{"test":"\\w+"}
    }
}
