{
    "outputMiddleware": {
        "class": "FxBot\\Model\\Middleware\\OutputMiddleware",
        "inject": [
            "@securityMiddleware@",
            "%defaultContentType%",
            "%assetsVersion%",
            "@sessionService@"
        ]
    },
    "securityMiddleware": {
        "class": "LightApp\\Model\\Middleware\\SecurityMiddleware",
        "inject": [
            "@controllerMiddleware@",
            "%securityList%",
            "@sessionService@"
        ]
    },
    "controllerMiddleware": {
        "class": "LightApp\\Model\\Middleware\\ControllerMiddleware",
        "inject": [
            "%routedController%",
            "%routedAction%"
        ]
    },




    "authenticationController": {
        "class": "FxBot\\Controller\\AuthenticationController",
        "inject": [
            "@sessionService@",
            "@validatorFactory@",
            "%inMemoryUsername%",
            "%inMemoryPasswordHash%"
        ]
    },
    "forexController": {
        "class": "FxBot\\Controller\\ForexController",
        "inject": [
            "@tradeService@"
        ]
    },




    "sessionService": {
        "class": "LightApp\\Model\\Service\\SessionService"
    },
    "validatorFactory": {
        "class": "LightApp\\Model\\Validator\\ValidatorFactory",
        "inject": [
            "@sessionService@"
        ]
    },
    "clientFactory": {
        "class": "HttpClient\\ClientFactory",
        "inject": [
            "%clientConfiguration%"
        ]
    },
    "oandaFetchingService": {
        "class": "FxBot\\Model\\Service\\OandaFetchingService",
        "inject": [
            "%priceInstruments%",
            "@priceService@",
            "@indicatorService@",
            "@clientFactory@"
        ]
    },
    "forexFactoryFetchingService": {
        "class": "FxBot\\Model\\Service\\ForexFactoryFetchingService",
        "inject": [
            "%priceInstruments%",
            "@indicatorService@",
            "@clientFactory@"
        ]
    },
    "priceService": {
        "class": "FxBot\\Model\\Service\\PriceService",
        "inject": [
            "@priceRepository@"
        ]
    },
    "indicatorService": {
        "class": "FxBot\\Model\\Service\\IndicatorService",
        "inject": [
            "@indicatorRepository@"
        ]
    },
    "tradeService": {
        "class": "FxBot\\Model\\Service\\TradeService",
        "inject": [
            "%priceInstruments%",
            "@clientFactory@",
            "%oandaAccount%",
            "%selectedStrategy%",
            "%strategyParams%",
            "@strategyFactory@",
            "@tradeRepository@"
        ]
    },
    "simulationService": {
        "class": "FxBot\\Model\\Service\\SimulationService",
        "inject": [
            "%priceInstruments%",
            "@priceService@",
            "@strategyFactory@",
            "@tradeRepository@",
            "@simulationRepository@"
        ]
    },
    "learningService": {
        "class": "FxBot\\Model\\Service\\LearningService",
        "inject": [
            "@simulationService@",
            "@learningRepository@"
        ]
    },
    "strategyFactory": {
        "class": "FxBot\\Model\\Strategy\\StrategyFactory",
        "inject": [
            "%priceInstruments%",
            "@priceService@",
            "@indicatorService@"
        ]
    },



    "readConnection": {
        "class": "LightApp\\Model\\Repository\\PdoDatabaseConnection",
        "inject": [
            "%databaseEngine%",
            "%databaseHost%",
            "%databasePort%",
            "%databaseName%",
            "%databaseUser%",
            "%databasePassword%"
        ]
    },
    "writeConnection": {
        "class": "LightApp\\Model\\Repository\\PdoDatabaseConnection",
        "inject": [
            "%databaseEngine%",
            "%databaseHost%",
            "%databasePort%",
            "%databaseName%",
            "%databaseUser%",
            "%databasePassword%"
        ]
    },
    "priceRepository": {
        "class": "FxBot\\Model\\Repository\\PriceRepository",
        "inject": [
            "@writeConnection@",
            "@writeConnection@"
        ]
    },
    "indicatorRepository": {
        "class": "FxBot\\Model\\Repository\\IndicatorRepository",
        "inject": [
            "@writeConnection@",
            "@writeConnection@"
        ]
    },
    "tradeRepository": {
        "class": "FxBot\\Model\\Repository\\TradeRepository",
        "inject": [
            "@writeConnection@",
            "@writeConnection@"
        ]
    },
    "simulationRepository": {
        "class": "FxBot\\Model\\Repository\\SimulationRepository",
        "inject": [
            "@writeConnection@",
            "@writeConnection@"
        ]
    },
    "learningRepository": {
        "class": "FxBot\\Model\\Repository\\LearningRepository",
        "inject": [
            "@writeConnection@",
            "@writeConnection@"
        ]
    },




    "populateRandomCommand": {
        "class": "FxBot\\Model\\Command\\PopulateRandomCommand",
        "inject": [
            "@priceService@"
        ]
    },
    "populatePricesCommand": {
        "class": "FxBot\\Model\\Command\\PopulatePricesCommand",
        "inject": [
            "@oandaFetchingService@"
        ]
    },
    "populateIndicatorsCommand": {
        "class": "FxBot\\Model\\Command\\PopulateIndicatorsCommand",
        "inject": [
            "@forexFactoryFetchingService@"
        ]
    },
    "tradeCommand": {
        "class": "FxBot\\Model\\Command\\TradeCommand",
        "inject": [
            "@tradeService@"
        ]
    },
    "simulationCommand": {
        "class": "FxBot\\Model\\Command\\SimulationCommand",
        "inject": [
            "@simulationService@"
        ]
    },
    "learningCommand": {
        "class": "FxBot\\Model\\Command\\LearningCommand",
        "inject": [
            "@learningService@"
        ]
    }
}
