{
    "outputMiddleware": {
        "class": "TinyApp\\Model\\Middleware\\OutputMiddleware",
        "inject": [
            "@securityMiddleware@",
            "%defaultContentType%",
            "%assetsVersion%",
            "@fileService@",
            "@sessionService@"
        ]
    },
    "securityMiddleware": {
        "class": "TinyApp\\Model\\Middleware\\SecurityMiddleware",
        "inject": [
            "@responseCacheMiddleware@",
            "%securityList%",
            "@sessionService@"
        ]
    },
    "responseCacheMiddleware": {
        "class": "TinyApp\\Model\\Middleware\\ResponseCacheMiddleware",
        "inject": [
            "@controllerMiddleware@",
            "%cacheList%"
        ]
    },
    "controllerMiddleware": {
        "class": "TinyApp\\Model\\Middleware\\ControllerMiddleware",
        "inject": [
            "%routedController%",
            "%routedAction%"
        ]
    },




    "fileController": {
        "class": "TinyApp\\Controller\\FileController",
        "inject": [
            "@fileService@",
            "@validatorFactory@",
            "@sessionService@"
        ]
    },
    "authenticationController": {
        "class": "TinyApp\\Controller\\AuthenticationController",
        "inject": [
            "@sessionService@",
            "@validatorFactory@",
            "%inMemoryUsername%",
            "%inMemoryPasswordHash%"
        ]
    },
    "privateContentController": {
        "class": "TinyApp\\Controller\\PrivateContentController",
        "inject": [
            "@sessionService@",
            "@fileService@"
        ]
    },
    "forexController": {
        "class": "TinyApp\\Controller\\ForexController"
    },




    "sessionService": {
        "class": "TinyApp\\Model\\Service\\SessionService"
    },
    "fileService": {
        "class": "TinyApp\\Model\\Service\\FileService",
        "inject": [
            "@fileRepository@"
        ]
    },
    "validatorFactory": {
        "class": "TinyApp\\Model\\Validator\\ValidatorFactory",
        "inject": [
            "@sessionService@"
        ]
    },
    "clientFactory": {
        "class": "HttpClient\\ClientFactory",
        "inject": [
            "%clientConfiguration%"
        ]
    },
    "oandaFetchingService": {
        "class": "TinyApp\\Model\\Service\\OandaFetchingService",
        "inject": [
            "%priceInstruments%",
            "@priceService@",
            "@indicatorService@",
            "@clientFactory@"
        ]
    },
    "forexFactoryFetchingService": {
        "class": "TinyApp\\Model\\Service\\ForexFactoryFetchingService",
        "inject": [
            "%priceInstruments%",
            "@indicatorService@",
            "@clientFactory@"
        ]
    },
    "priceService": {
        "class": "TinyApp\\Model\\Service\\PriceService",
        "inject": [
            "@priceRepository@"
        ]
    },
    "indicatorService": {
        "class": "TinyApp\\Model\\Service\\IndicatorService",
        "inject": [
            "@indicatorRepository@"
        ]
    },
    "tradeService": {
        "class": "TinyApp\\Model\\Service\\TradeService",
        "inject": [
            "%priceInstruments%",
            "@clientFactory@",
            "%oandaAccount%",
            "%selectedStrategy%",
            "@strategyFactory@",
            "@tradeRepository@"
        ]
    },
    "simulationService": {
        "class": "TinyApp\\Model\\Service\\SimulationService",
        "inject": [
            "%priceInstruments%",
            "@priceService@",
            "@strategyFactory@",
            "@tradeRepository@",
            "@simulationRepository@"
        ]
    },
    "strategyFactory": {
        "class": "TinyApp\\Model\\Strategy\\StrategyFactory",
        "inject": [
            "%priceInstruments%",
            "@priceService@",
            "@indicatorService@"
        ]
    },




    "writeConnection": {
        "class": "TinyApp\\Model\\Repository\\DatabaseConnection",
        "inject": [
            "%databaseEngine%",
            "%databaseHost%",
            "%databaseName%",
            "%databaseUser%",
            "%databasePassword%"
        ]
    },
    "fileRepository" : {
        "class": "TinyApp\\Model\\Repository\\FileRepository",
        "inject": [
            "@writeConnection@"
        ]
    },
    "priceRepository": {
        "class": "TinyApp\\Model\\Repository\\PriceRepository",
        "inject": [
            "@writeConnection@"
        ]
    },
    "indicatorRepository": {
        "class": "TinyApp\\Model\\Repository\\IndicatorRepository",
        "inject": [
            "@writeConnection@"
        ]
    },
    "tradeRepository": {
        "class": "TinyApp\\Model\\Repository\\TradeRepository",
        "inject": [
            "@writeConnection@"
        ]
    },
    "simulationRepository": {
        "class": "TinyApp\\Model\\Repository\\SimulationRepository",
        "inject": [
            "@writeConnection@"
        ]
    },




    "populateRandomCommand": {
        "class": "TinyApp\\Model\\Command\\PopulateRandomCommand",
        "inject": [
            "@priceService@"
        ]
    },
    "populatePricesCommand": {
        "class": "TinyApp\\Model\\Command\\PopulatePricesCommand",
        "inject": [
            "@oandaFetchingService@"
        ]
    },
    "populateIndicatorsCommand": {
        "class": "TinyApp\\Model\\Command\\PopulateIndicatorsCommand",
        "inject": [
            "@forexFactoryFetchingService@"
        ]
    },
    "tradeCommand": {
        "class": "TinyApp\\Model\\Command\\TradeCommand",
        "inject": [
            "@tradeService@"
        ]
    },
    "simulationCommand": {
        "class": "TinyApp\\Model\\Command\\SimulationCommand",
        "inject": [
            "@simulationService@"
        ]
    }
}
